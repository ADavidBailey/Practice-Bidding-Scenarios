Script,OppsOvercallStaJac
window.currentPBSScenario = 'OppsOvercallStaJac';
window.currentPBSScenarioFilename = 'Opps_Overcall_Stayman_or_Jacoby';
console.log('PBS: Logging scenario selection for OppsOvercallStaJac');
fetch('https://bba.harmonicsystems.com/api/scenario/select', {
    method: 'POST',
    headers: {
        'Content-Type': 'application/json',
        'X-Client-Version': '1.0.0'
    },
    body: JSON.stringify({
        scenario: 'OppsOvercallStaJac',
        user: whoAmI() || 'anonymous'
    })
}).then(function(r){ console.log('PBS: Server response', r.status); }).catch(function(e){ console.log('PBS: Fetch error', e); });
setDealerCode(`

/*
auction-filter: Auction.....\n1N +Pass +2[CDH] +[123]
*/
# Opponent overcalls stayman or jacoby
dealer south


# Define Good suits east
Gc = clubs(east)>5 and top4(east,clubs)==3
Gd = diamonds(east)>5 and top4(east,diamonds)==3
Gh = hearts(east)>5 and top4(east,hearts)==3
Gs = spades(east)>5 and top4(east,spades)==3

# South opens 1N
shape(south, any 4333 +any 4432 +any 5332) and
hcp(south)>14 and hcp(south)<18 and

# North bids Jacoby or Stayman
(
# Jacoby - no hcp required
(
(spades(north)>4 and (Gc or Gd or Gh))
or
(hearts(north)>4 and (Gc or Gd or Gs))
)
or
# Stayman 
shape(north, 54xx+45xx+44xx+4xxx+x4xx) and hcp(north)>8 and (Gc or Gd or Gh or Gs)
) and

# East has values
hcp(east)>10
`, "S", true)
Script
Button,Overcall S/J,\n\
--- Opponent overcalls our Stayman/Jacoby bid\n\
%OppsOvercallStaJac%,width=50%
