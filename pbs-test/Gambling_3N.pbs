Script,Gambling3N
window.currentPBSScenario = 'Gambling3N';
window.currentPBSScenarioFilename = 'Gambling_3N';
console.log('PBS: Logging scenario selection for Gambling3N');
fetch('https://bba.harmonicsystems.com/api/scenario/select', {
    method: 'POST',
    headers: {
        'Content-Type': 'application/json',
        'X-Client-Version': '1.0.0'
    },
    body: JSON.stringify({
        scenario: 'Gambling3N',
        user: whoAmI() || 'anonymous'
    })
}).then(function(r){ console.log('PBS: Server response', r.status); }).catch(function(e){ console.log('PBS: Fetch error', e); });
setDealerCode(`

/*
auction-filter: Auction.....\n3NT? +
*/
dealer south

# Gambling 3N
hcp(south)>8 and hcp(south)<12  // could have a Q or two J's outside
and
(
(top3(south,diamonds)==3 and diamonds(south)>6)
or
(top3(south,clubs)==3 and clubs(south)>6)
)
and shape(south,any 9xxx +any 8xxx +any 7xxx -any 4xxx -any 5xxx -any 6xxx)
`, "S", true)
