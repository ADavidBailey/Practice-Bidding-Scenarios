Script,OppsPreempt
window.currentPBSScenario = 'OppsPreempt';
console.log('PBS: Logging scenario selection for OppsPreempt');
fetch('https://bba.harmonicsystems.com/api/scenario/select', {
    method: 'POST',
    headers: {
        'Content-Type': 'application/json',
        'X-Client-Version': '1.0.0'
    },
    body: JSON.stringify({
        scenario: 'OppsPreempt',
        user: whoAmI() || 'anonymous'
    })
}).then(function(r){ console.log('PBS: Server response', r.status); }).catch(function(e){ console.log('PBS: Fetch error', e); });
setDealerCode(`

/*
auction-filter: Auction.....\\n[234567][CDHS]
*/
# Opps Preempt
dealer east

hcp(east)>5 and hcp(east)<11
and
(
(top5(east,spades)>1 and spades(east)>6)
or
(top5(east,hearts)>1 and hearts(east)>6)
or
(top5(east,diamonds)>1 and diamonds(east)>6)
or
(top5(east,clubs)>1 and clubs(east)>6)
)
`, "E", true)
Script
Button,Preempt,\n\
--- Opponents Preempt\n\
%OppsPreempt%
