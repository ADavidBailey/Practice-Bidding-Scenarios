Script,scs161or2C
window.currentPBSScenario = 'scs161or2C';
window.currentPBSScenarioFilename = 'SCS16_1or2C';
console.log('PBS: Logging scenario selection for scs161or2C');
fetch('https://bba.harmonicsystems.com/api/scenario/select', {
    method: 'POST',
    headers: {
        'Content-Type': 'application/json',
        'X-Client-Version': '1.0.0'
    },
    body: JSON.stringify({
        scenario: 'scs161or2C',
        user: whoAmI() || 'anonymous'
    })
}).then(function(r){ console.log('PBS: Server response', r.status); }).catch(function(e){ console.log('PBS: Fetch error', e); });
setDealerCode(`

/*
convention-card: Precision
auction-filter: Auction.....\n[12]C
*/
# convention-card: Precision


/*
Strong Club System

1C (16+) or 2C(11-15)

convention-card: Precision
auction-filter: Auction.....\n[12]C
*/

dealer south

s = spades(south)
h = hearts(south)
d = diamonds(south)
c = clubs(south)

range1C = hcp(south)>15
range2C = hcp(south)>10 and hcp(south)<16
range = range1C or range2C
suit = clubs(south)>5 and clubs(south)>diamonds(south)

range and suit
`, "S", true)
