Script,GoingForBlood
window.currentPBSScenario = 'GoingForBlood';
window.currentPBSScenarioFilename = 'Going_for_Blood';
console.log('PBS: Logging scenario selection for GoingForBlood');
fetch('https://bba.harmonicsystems.com/api/scenario/select', {
    method: 'POST',
    headers: {
        'Content-Type': 'application/json',
        'X-Client-Version': '1.0.0'
    },
    body: JSON.stringify({
        scenario: 'GoingForBlood',
        user: whoAmI() || 'anonymous'
    })
}).then(function(r){ console.log('PBS: Server response', r.status); }).catch(function(e){ console.log('PBS: Fetch error', e); });
setDealerCode(`

